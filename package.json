{
  "name": "antd-form-render",
  "version": "1.1.1",
  "description": "基于react hooks, antd4 , js对象配置的表单渲染方案",
  "keywords": [
    "react",
    "antd",
    "form",
    "formrender"
  ],
  "license": "MIT",
  "repository": "https://github.com/leonwgc/antd-form-render",
  "bugs": {
    "url": "https://github.com/leonwgc/antd-form-render/issues"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "types/index.d.ts",
  "scripts": {
    "start": "webpack-dev-server --dev",
    "clear": "rm -rf ./lib && rm -rf ./es && clear",
    "types": "tsc && mkdir ./lib && mkdir ./es && cp ./index.d.ts ./lib/ && cp ./index.d.ts ./es/",
    "build:cjs": "TARGET=cjs babel src  -x '.tsx','.jsx' -d lib",
    "build:mjs": "TARGET=mjs babel src  -x '.tsx','.jsx'  -d es",
    "build": "yarn clear && father-build --cjs --esm && tsc",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "peerDependencies": {
    "antd": ">=4.0.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "devDependencies": {
    "antd": "^4.6.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-stage-0": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@types/react": "^16.8.2",
    "@types/react-dom": "^16.8.0",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "antd-dayjs-webpack-plugin": "^1.0.1",
    "autoprefixer": "^9.7.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-require-ignore": "^0.1.1",
    "chalk": "^4.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^0.28.5",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "father-build": "^1.19.4",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^4.0.3",
    "husky": "^6.0.0",
    "less": "^3.0.1",
    "less-loader": "^4.0.3",
    "lint-staged": "^10.5.4",
    "mini-css-extract-plugin": "^0.4.0",
    "optimize-css-assets-webpack-plugin": "^4.0.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^4.1.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "style-loader": "^0.20.3",
    "typescript": "^4.2.4",
    "url-loader": "^1.0.1",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.2",
    "webpackbar": "^4.0.0",
    "yargs": "^15.3.1"
  },
  "files": [
    "dist",
    "types",
    "es"
  ],
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  }
}
